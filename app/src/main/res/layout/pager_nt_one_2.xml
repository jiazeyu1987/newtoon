<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1080dp"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true" >

    <ImageView
        android:id="@+id/imageView42"
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/customItemA6"
        app:layout_constraintTop_toBottomOf="@+id/customItem56"
        app:srcCompat="@drawable/card" />

    <com.example.administrator.new_ptns.custom_item.CustomItem5
        android:id="@+id/customItem56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:txt1="阻抗测量">

    </com.example.administrator.new_ptns.custom_item.CustomItem5>

    <com.example.administrator.new_ptns.custom_item.CustomItemA6
        android:id="@+id/customItemA6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView42" />

    <com.example.administrator.new_ptns.custom_item.CustomItemA6
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        app:txt1 = "电极二"
        app:txt2 = "RSTN"
        app:layout_constraintBottom_toBottomOf="@+id/customItemA6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/customItemA6" />
</android.support.constraint.ConstraintLayout>
