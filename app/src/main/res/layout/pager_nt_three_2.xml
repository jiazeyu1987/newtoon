        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cons1traintLayout111">

        <Button
            android:id="@+id/btn_stim_report"
            style="@style/RoundRectButton2"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:text="刺激日志"
            app:layout_constraintStart_toEndOf="@+id/btn_operation_report"
            app:layout_constraintTop_toBottomOf="@+id/customI1t1em512" />

        <Button
            android:id="@+id/btn_impedance_report"
            style="@style/RoundRectButton2"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
