<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="136dp"
    android:paddingBottom="5dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="131dp"
        android:background="#ffffff">

        <LinearLayout
            android:id="@+id/linearLayout7"
            android:layout_width="335dp"
            android:layout_height="1dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="8dp"
            android:background="#F0F0F0"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></LinearLayout>

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="@+id/linearLayout7"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/plate" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:fontFamily="@font/ping"
            android:text="模板1"
            android:textColor="#333333"
            android:textSize="13sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView28"
            app:layout_constraintTop_toTopOf="@+id/imageView28" />

        <TextView
            android:id="@+id/textView47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ping"
            android:text="30分钟"
            android:textColor="#005A8A"
            android:textSize="13sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView46"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout7"
            app:layout_constraintTop_toTopOf="@+id/textView46" />

        <TextView
            android:id="@+id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/ping"
            android:text="治疗时长："
            android:textColor="#AFB7BC"
            android:textSize="13sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView46"
            app:layout_constraintEnd_toStartOf="@+id/textView47"
            app:layout_constraintTop_toTopOf="@+id/textView46" />

        <ImageView
            android:id="@+id/imageView29"
            android:layout_width="72dp"
            android:layout_height="38dp"
            android:layout_marginBottom="21dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/more2" />

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="54dp"
            android:layout_height="18dp"
            android:layout_marginEnd="27dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintEnd_toStartOf="@+id/imageView29"
            app:srcCompat="@drawable/p2" />

        <ImageView
            android:id="@+id/imageView31"
            android:layout_width="54dp"
            android:layout_height="18dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintEnd_toStartOf="@+id/imageView30"
            app:layout_constraintStart_toEndOf="@+id/imageView32"
            app:srcCompat="@drawable/f2" />

        <ImageView
            android:id="@+id/imageView32"
            android:layout_width="54dp"
            android:layout_height="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintStart_toStartOf="@+id/linearLayout7"
            app:srcCompat="@drawable/c2" />

        <TextView
            android:id="@+id/textView49"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ping"
            android:text="34.00"
            android:textColor="#333333"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toStartOf="@+id/imageView32" />

        <TextView
            android:id="@+id/textView52"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:fontFamily="@font/ping"
            android:text="mA"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toEndOf="@+id/textView49" />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:fontFamily="@font/ping"
            android:text="Hz"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toEndOf="@+id/textView50" />

        <TextView
            android:id="@+id/textView54"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:fontFamily="@font/ping"
            android:text="us"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toEndOf="@+id/textView51" />

        <TextView
            android:id="@+id/textView50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ping"
            android:text="156"
            android:textColor="#333333"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toStartOf="@+id/imageView31" />

        <TextView
            android:id="@+id/textView51"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ping"
            android:text="6"
            android:textColor="#333333"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintStart_toStartOf="@+id/imageView30" />
    </android.support.constraint.ConstraintLayout>
</LinearLayout>