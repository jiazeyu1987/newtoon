<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/pager_content_width"
    android:layout_height="@dimen/pager_content_height"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <ScrollView
        android:layout_width="@dimen/pager_content_width"
        android:layout_height="@dimen/pager_content_height"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/adfad1f1asdf"
                    android:layout_width="470dp"
                    android:layout_height="300dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout10">

                    <com.example.administrator.new_ptns.custom_item.CustomItemA8
                        android:id="@+id/customItemA83"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="@+id/customItemA82"
                        app:layout_constraintHorizontal_bias="0.466"
                        app:layout_constraintStart_toStartOf="@+id/customItemA82"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA82"></com.example.administrator.new_ptns.custom_item.CustomItemA8>

                    <com.example.administrator.new_ptns.custom_item.CustomItemA8
                        android:id="@+id/customItemA82"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"></com.example.administrator.new_ptns.custom_item.CustomItemA8>

                    <com.example.administrator.new_ptns.custom_item.CustomItemA8
                        android:id="@+id/customItemA84"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA83"></com.example.administrator.new_ptns.custom_item.CustomItemA8>

                    <com.example.administrator.new_ptns.custom_item.CustomItemA8
                        android:id="@+id/customItemA85"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA84"></com.example.administrator.new_ptns.custom_item.CustomItemA8>

                    <com.example.administrator.new_ptns.custom_item.CustomItemA8
                        android:id="@+id/customItemA18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA85"></com.example.administrator.new_ptns.custom_item.CustomItemA8>

                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/adfad11fasdf"
                    android:layout_width="600dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout10">

                    <com.example.administrator.new_ptns.custom_item.CustomItemA7
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="16dp"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA7"
                        app:txt1="夜间" />

                    <com.example.administrator.new_ptns.custom_item.CustomItemA7
                        android:id="@+id/customItemA7"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/adfad1fasdf"
                    android:layout_width="470dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:id="@+id/customItemA3p13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:btn1="禁止"
                        app:layout_constraintStart_toStartOf="@+id/textView113"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA3p12"
                        app:txt1="频率" />

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:id="@+id/customItemA3p14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:btn1="禁止"
                        app:layout_constraintStart_toStartOf="@+id/textView113"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA3p13"
                        app:txt1="脉宽" />

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp"
                        app:btn1="允许"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/textView113"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA3p14"
                        app:txt1="持续时间" />

                    <TextView
                        android:id="@+id/textView113"
                        style="@style/SmallText1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="16dp"
                        android:text="患者程控范围"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:id="@+id/customItemA3p12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        app:btn1="禁止"
                        app:layout_constraintStart_toStartOf="@+id/textView113"
                        app:layout_constraintTop_toBottomOf="@+id/textView113"
                        app:txt1="幅度" />
                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/adfadfasdf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="200dp"
                        app:btn1="关闭"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/customItemA3p1"
                        app:txt1="软停止" />

                    <com.example.administrator.new_ptns.custom_item.CustomItemA3p1
                        android:id="@+id/customItemA3p1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="200dp"
                        app:btn1="关闭"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:txt1="软启动" />

                    <com.kyleduo.switchbutton.SwitchButton
                        android:id="@+id/switch2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="300dp"
                        android:scaleX="1.5"
                        android:scaleY="1.5"
                        android:textOff="恒流 mA"
                        android:textOn="恒压 V"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/constraintLayout10"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/adfadfasdf">

                    <Button
                        android:id="@+id/button41"
                        style="@style/RoundRectButton2"
                        android:layout_width="500dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="16dp"
                        android:text="LSTN"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView111"
                        style="@style/SmallText1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginBottom="16dp"
                        android:text="刺激通道"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <TextView
                        android:id="@+id/textView112"
                        style="@style/SmallText1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp"
                        android:text="刺激参数"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/button40" />

                    <Button
                        android:id="@+id/button40"
                        style="@style/RoundRectButton2"
                        android:layout_width="500dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:text="RSTN"
                        app:layout_constraintStart_toEndOf="@+id/button41"
                        app:layout_constraintTop_toTopOf="parent" />

                    <CheckBox
                        android:id="@+id/checkBox6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp"
                        android:text="复合方案"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/button40" />

                    <CheckBox
                        android:id="@+id/checkBox7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:text="实时刺激"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/checkBox6" />
                </android.support.constraint.ConstraintLayout>

            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button422"
                style="@style/RoundRectButton2"
                android:text="开始治疗" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button412"
                style="@style/RoundRectButton2"
                android:text="保存报告" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button42"
                style="@style/RoundRectButton2"
                android:text="刺激器关机" />
        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
